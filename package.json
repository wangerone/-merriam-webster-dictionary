{
  "$schema": "https://www.raycast.com/schemas/extension.json",
  "name": "merriam-webster-dictionary",
  "title": "Merriam-Webster Dictionary",
  "description": "Merriam-Webster Dictionary",
  "icon": "command-icon.png",
  "author": "wangerone",
  "categories": [
    "Productivity"
  ],
  "license": "MIT",
  "commands": [
    {
      "name": "merriam-webster-dictionary",
      "title": "Merriam-Webster Dictionary",
      "description": "",
      "mode": "view",
      "preferences": [
        {
          "name": "MW_DICT_API_KEY",
          "description": "API KEY FROM https://dictionaryapi.com/",
          "type": "password",
          "required": true,
          "placeholder": "API KEY FROM https://dictionaryapi.com/"
        }
      ]
    }
  ],
  "dependencies": {
    "@raycast/api": "^1.48.9",
    "@raycast/utils": "^1.5.2"
  },
  "devDependencies": {
    "@raycast/eslint-config": "1.0.5",
    "@types/node": "18.8.3",
    "@types/react": "18.0.9",
    "eslint": "^7.32.0",
    "prettier": "^2.5.1",
    "typescript": "^4.4.3"
  },
  "scripts": {
    "build": "ray build -e dist",
    "dev": "ray develop",
    "fix-lint": "ray lint --fix",
    "lint": "ray lint",
    "publish": "ray publish"
  }
}